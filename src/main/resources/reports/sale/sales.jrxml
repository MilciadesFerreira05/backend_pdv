	<queryString language="SQL">
		<![CDATA[SELECT 
	products_sales.id,
	products.name productname,
	products.code,
	sale_items.price,
	sale_items.quantity,
	sale_items.subtotal,
	products_sales.total,
	products_sales.date,
	products_sales.invoice_number,
	clients.name as clientname,
	users.username
FROM sale_items
	INNER JOIN products_sales ON 
	 sale_items.sale_id = products_sales.id 
	INNER JOIN products ON 
	 sale_items.product_id = products.id 
	INNER JOIN clients ON 
	 products_sales.client_id = clients.id 
	INNER JOIN users ON 
	 products_sales.created_by = users.id 
	 WHERE 
	 products_sales.deleted_at IS NULL  
	 AND products_sales.date BETWEEN $P{desde} AND $P{hasta}
]]>
	</queryString>
